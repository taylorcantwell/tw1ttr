    "@types/dropzone": "5.7.4",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/uuid": "8.3.4",
    "autoprefixer": "10.4.7",
    "concurrently": "7.2.2",
    "eslint": "8.18.0",
