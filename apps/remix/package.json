{
  "name": "remix",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "build": "yarn run build:css && remix build",
    "build:css": "tailwindcss -m -i ./app/styles/tailwind.css -o ./app/styles/styles.css",
    "dev": "concurrently \"npm run dev:css\" \"cross-env NODE_ENV=development netlify dev\"",
    "start": "cross-env NODE_ENV=production netlify dev",
    "dev:css": "tailwindcss -m -i ./app/styles/tailwind.css -o ./app/styles/styles.css"
  },
  "dependencies": {
    "cross-env": "7.0.3",
    "@netlify/functions": "1.0.0",
    "@remix-run/netlify": "1.6.1",
    "@remix-run/node": "1.6.1",
    "@remix-run/react": "1.6.1",
    "@heroicons/react": "1.0.6",
    "@radix-ui/react-dialog": "0.1.7",
    "@radix-ui/react-popover": "0.1.6",
    "@radix-ui/react-toast": "0.1.1",
    "@remix-validated-form/with-zod": "2.0.1",
    "@twitter-clone/prisma": "*",
    "clsx": "1.1.1",
    "dayjs": "1.11.3",
    "framer-motion": "6.3.15",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.1",
    "react-textarea-autosize": "8.3.4",
    "react-use": "17.4.0",
    "react-virtual": "2.10.4",
